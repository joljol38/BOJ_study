    "coin = []\n",
    "new = []\n",
    "count = 0\n",
    "N, K = map(int, input().split())\n",
    "\n",
    "for i in range(N):\n",
    "    c = int(input())\n",
    "    coin.append(c)\n",
    "    new = coin[::-1]\n",
    "# print(new)\n",
    "\n",
    "for coin in new:\n",
    "    count += K // coin\n",
    "    K %= coin\n",
    "print(count)\n"
   ]
