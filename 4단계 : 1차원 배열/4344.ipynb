   "source": [
    "a = int(input())\n",
    "\n",
    "for i in range(a):\n",
    "    b = list(map(int, input().split()))\n",
    "    c = sum(b[1:])/b[0]\n",
    "    cnt = 0\n",
    "    \n",
    "    for j in b[1:]:\n",
    "        if j > c:\n",
    "            cnt += 1\n",
    "    percentage = cnt/b[0] * 100\n",
    "    print(f\"{percentage:.3f}%\")"
   ]
  }
