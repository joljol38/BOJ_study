    "def hansu(n):\n",
    "    cnt = 0\n",
    "    \n",
    "    for i in range(1, n+1):\n",
    "        a = list(map(int,str(i)))\n",
    "        if i < 100:\n",
    "            cnt += 1\n",
    "        elif a[0]-a[1] == a[1]-a[2]:\n",
    "            cnt += 1\n",
    "    return cnt\n",
    "\n",
    "n = int(input())\n",
    "print(hansu(n))"
   ]
  }
