   "source": [
    "N = int(input())\n",
    "num = N\n",
    "cy = 0\n",
    "\n",
    "while True:\n",
    "    a = num // 10\n",
    "    b = num % 10\n",
    "    c = (a + b) % 10\n",
    "    num = (b * 10) + c\n",
    "    \n",
    "    cy += 1\n",
    "    \n",
    "    if (num == N):\n",
    "        break\n",
    "    \n",
    "print(cy)\n"
   ]
  }
